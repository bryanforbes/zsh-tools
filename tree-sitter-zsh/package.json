{
  "name": "tree-sitter-zsh",
  "version": "0.1.0",
  "description": "Zsh grammar for tree-sitter",
  "repository": "https://github.com/bryanforbes/tree-sitter-zsh",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Bryan Forbes",
    "email": "bryan@reigndropsfall.net",
    "url": "https://www.reigndropsfall.net/"
  },
  "type": "module",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "zsh"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test:grammar": "tree-sitter test",
    "test:bindings": "node --test bindings/node/*_test.js",
    "test": "npm run test:grammar && npm run test:bindings"
  }
}
